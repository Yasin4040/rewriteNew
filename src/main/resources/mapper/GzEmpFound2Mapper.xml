    <update id="updateBatchForSchool" parameterType="java.util.List">
        <foreach collection="dtoList" item="item" index="index" separator=";">
            UPDATE GZ_RY_XL
            SET xxmc = #{item.value}
            WHERE xxmc = #{item.name}
