    <select id="listSimple" resultType="com.lizy.rewritenew.entity.OriginSimple">
        WITH RankedZW AS (
            SELECT
                grz.*,grj.SFZH,grj.xm,
                ROW_NUMBER() OVER (PARTITION BY grz.RYBM ORDER BY grz.ZXSJ) AS rn
            FROM
                GZ_RY_ZW grz inner JOIN
                GZ_RY_JBXX grj ON grj.rybm = grz.rybm and grj.ryzt in ('01','02',' ') and grj.BZXZ in  ('01','02',' ')
        )
        select grj.ryxh, grj.rybm as rybm,grj.dwbm,gdd.DWMC as dwbmStr,grj.xm,grj.sfzh,grj.csny,grj.cjgzsj,grj.jrdwsj,r1.srzwmc
        from GZ_RY_JBXX grj left join GZ_DW_DWXX gdd on gdd.DWBM  = grj.DWBM
                            left join RankedZW r1 on r1.rybm = grj.rybm and rn = 1
        where grj.ryzt in ('01','02') and grj.BZXZ in  ('01','02',' ')
        order by grj.rybm
    </select>
    <select id="listDistinctMc" resultType="java.lang.String">
        select distinct srzwmc from GZ_RY_ZW
